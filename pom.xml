    <dependency>
      <groupId>com.h2database</groupId>
      <artifactId>h2</artifactId>
      <version>2.1.212</version>
    </dependency>
  </dependencies>

  <build>
    <finalName>LocalProblemSolver</finalName>
  </build>
</project>
